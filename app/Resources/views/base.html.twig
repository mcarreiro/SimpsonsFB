                  <meta property="og:site_name" content="simpsonsFB"/>
	<meta property="og:type" content="blog"/>
	<meta property="og:description" content="{% if entity is defined%}{{entity.nombre}}{%else%} Todas las frases de los simpsons! {% endif %}"/>
